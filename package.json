{
  "name": "eslint-plugin-lodash",
  "version": "0.1.2",
  "description": "ESLint Plugin for use with the lodash library",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib"
